{
  "name": "@blendduck/node-sdk",
  "version": "0.0.1",
  "description": "The official Node.js library for the BlendDuck API",
  "author": "BlendDuck <support@blendduck.com>",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "type": "commonjs",
  "repository": "github:blendduck/blendduck-node",
  "license": "MIT",
  "packageManager": "yarn@1.22.22",
  "files": [
    "dist",
    "src"
  ],
  "private": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rimraf dist",
    "api": "ts-node scripts/openapi.ts",
    "jimu": "ts-node scripts/jimu.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn clean && yarn build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^7.1.1",
    "@babel/preset-env": "^7.25.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.2",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "sideEffects": false,
  "imports": {},
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "nanoid": "^3",
    "node-fetch": "^3.3.2",
    "zod": "^3.23.8"
  }
}
